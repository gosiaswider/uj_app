<h1>Listing articles</h1>
 
<ul>
  <% @articles.each do |article| %>
  <li><%= link_to article.title, article_path(article) %></li>
  <% end %>
</ul>

<table>
  <tr>
    <th>Title</th>   
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New article', new_article_path %>